{
    "name": "icecave/siphon",
    "description": "A client for SDI XML feeds.",
    "keywords": [
        "sports",
        "direct",
        "xml",
        "feed",
        "sdi"
    ],
    "homepage": "https://github.com/IcecaveStudios/siphon",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=5.5",
        "eloquent/enumeration": "^5",
        "guzzlehttp/guzzle": "^6",
        "icecave/chrono": "^1.0.3",
        "react/event-loop": "^0.4",
        "wyrihaximus/react-guzzle-psr7": "^1"
    },
    "require-dev": {
        "eloquent/phony": "^0.6",
        "icecave/archer": "dev-develop",
        "phpunit/phpunit": "^4",
        "sami/sami": "^3"
    },
    "config": {
        "platform": {
            "php": "5.5.999"
        }
    },
    "autoload": {
        "psr-4": {
            "Icecave\\Siphon\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.0.x-dev"
        }
    }
}
